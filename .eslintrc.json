{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-plusplus": "off",
    "consistent-return": "off",
    "no-return-assign": "off",
    "func-names": "off",
    "no-console": "off"
  }
}
